<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        android:background="@color/session"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content">

    <TextView
            android:id="@+id/session_title_time"
            xmlns:android="http://schemas.android.com/apk/res/android"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="@dimen/global_text_size"
            android:textColor="@color/white"
            android:layout_marginTop="@dimen/global_top_padding"
            android:layout_marginLeft="@dimen/global_left_padding"
            android:layout_marginRight="@dimen/global_right_padding"
            android:layout_marginBottom="@dimen/global_bottom_padding">
    </TextView>

</LinearLayout>